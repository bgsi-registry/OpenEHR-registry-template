                        <term_definitions code="at0000">
                            <items id="text">Contraceptive use summary</items>
                            <items id="description">Summary and persistent information about the use of methods to prevent pregnancy.</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                            <items id="text">Tree</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                            <items id="text">Current user</items>
                            <items id="description">Individual currently uses contraception.</items>
                        </term_definitions>
                        <term_definitions code="at0005">
                            <items id="text">Not current user</items>
                            <items id="description">Individual has previously used contraception but is not a current user.</items>
                        </term_definitions>
                        <term_definitions code="at0006">
                            <items id="text">Never used</items>
                            <items id="description">Individual has never used any type of contraception.</items>
                        </term_definitions>
                        <term_definitions code="at0013">
                            <items id="text">Episode start date</items>
                            <items id="description">Date when this episode commenced.</items>
                            <items id="comment">Can be a partial date, for example, only a year.</items>
                        </term_definitions>
                        <term_definitions code="at0019">
                            <items id="text">Overall comment</items>
                            <items id="description">Additional narrative about the use of contraception that has not been captured in other fields.</items>
                        </term_definitions>
                        <term_definitions code="at0021">
                            <items id="text">Tree</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0022">
                            <items id="text">Last updated</items>
                            <items id="description">The date this contraceptive use summary was last updated.</items>
                        </term_definitions>
                        <term_definitions code="at0023">
                            <items id="text">Specific name</items>
                            <items id="description">Identification of the specific contraception used in this episode, by name.</items>
                            <items id="comment">For example: the brand name of the oral contraceptive pill. This data element is most likely redundant in the context of use of some contraceptives such as condoms where specification of the brand or type is usually not relevant.</items>
                        </term_definitions>
                        <term_definitions code="at0025">
                            <items id="text">Goal</items>
                            <items id="description">Intended outcome as a result of use of the specified contraception.</items>
                            <items id="comment">For example: pregnancy prevention, regular cycle or relief of pain. Coding with a terminology is recommended, where possible.</items>
                        </term_definitions>
                        <term_definitions code="at0026">
                            <items id="text">Episode details</items>
                            <items id="description">Additional structured details about the specified episode of contraception use.</items>
                        </term_definitions>
                        <term_definitions code="at0029">
                            <items id="text">Per type</items>
                            <items id="description">Details about use of a specified type of contraception.</items>
                        </term_definitions>
                        <term_definitions code="at0030">
                            <items id="text">Description</items>
                            <items id="description">Narrative description about the overall use of contraception during this episode.</items>
                            <items id="comment">This data element may be used to describe the pattern of use, including regularity of use. For example, the oral contraceptive pill was missed frequently, condoms used occasionally or to describe minor side effects.</items>
                        </term_definitions>
                        <term_definitions code="at0043">
                            <items id="text">Overall description</items>
                            <items id="description">Narrative summary about the individual's overall use of contraception.</items>
                            <items id="comment">Use this data element to record a narrative description where the structured data does not adequately reflect the use of contraception for this individual or to incorporate unstructured information from existing or legacy clinical systems into an archetyped format.</items>
                        </term_definitions>
                        <term_definitions code="at0053">
                            <items id="text">Description</items>
                            <items id="description">Narrative summary about the use of the specified type of contraception.</items>
                        </term_definitions>
                        <term_definitions code="at0064">
                            <items id="text">Per episode</items>
                            <items id="description">Details about contraceptive use during an identified period of time.</items>
                        </term_definitions>
                        <term_definitions code="at0069">
                            <items id="text">Comment</items>
                            <items id="description">Additional narrative about use of the specified type of contraception, not captured in other fields.</items>
                        </term_definitions>
                        <term_definitions code="at0073">
                            <items id="text">Extension</items>
                            <items id="description">Additional information required to capture local content or to align with other reference models/formalisms.</items>
                            <items id="comment">For example: local information requirements or additional metadata to align with FHIR.</items>
                        </term_definitions>
                        <term_definitions code="at0074">
                            <items id="text">Reason for cessation</items>
                            <items id="description">Narrative description for the reason for stopping use of the specified type of contraception during this episode.</items>
                            <items id="comment">For example: Major side effects or the wish to become pregnant. Coding with a terminology is recommended, where possible.</items>
                        </term_definitions>
                        <term_definitions code="at0077">
                            <items id="text">Type details</items>
                            <items id="description">Additional structured details about the specified type of contraception used.</items>
                        </term_definitions>
                        <term_definitions code="at0081">
                            <items id="text">Episode label</items>
                            <items id="description">Identification of an episode of contraception use - either as a number in a sequence and/or a named event.</items>
                            <items id="comment">For example: '2' as the second episode within a sequence of episodes; or 'Breastfeeding'.</items>
                        </term_definitions>
                        <term_definitions code="at0082">
                            <items id="text">Episode end date</items>
                            <items id="description">Date when this episode ceased.</items>
                            <items id="comment">Can be a partial date, for example, only a year. This data field will be empty if the episode is current and ongoing.</items>
                        </term_definitions>
                        <term_definitions code="at0087">
                            <items id="text">Episode comment</items>
                            <items id="description">Additional narrative about the use of the specified contraception during the specified episode, not captured in other fields.</items>
                        </term_definitions>
                        <term_definitions code="at0089">
                            <items id="text">Riwayat penggunaan KB</items>
                            <items id="description">Statement about regular use of any type of contraception.</items>
                        </term_definitions>
                        <term_definitions code="at0144">
                            <items id="text">Status</items>
                            <items id="description">Statement about current use of the specified type of contraception.</items>
                        </term_definitions>
                        <term_definitions code="at0145">
                            <items id="text">Current user</items>
                            <items id="description">Individual is a current user of the specified type of contraception.</items>
                        </term_definitions>
                        <term_definitions code="at0146">
                            <items id="text">Not current user</items>
                            <items id="description">Individual has previously used the specified type of contraception but is not a current user.</items>
                        </term_definitions>
                        <term_definitions code="at0147">
                            <items id="text">Never used</items>
                            <items id="description">Individual has never used the specified type of contraception.</items>
                        </term_definitions>
                        <term_definitions code="at0148">
                            <items id="text">Start date</items>
                            <items id="description">Date when the individual first used the specified type of contraception.</items>
                            <items id="comment">Comment: Can be a partial date, for example, only a year.</items>
                        </term_definitions>
                        <term_definitions code="at0149">
                            <items id="text">End date</items>
                            <items id="description">Date when the individual last used the specified type of contraception.</items>
                            <items id="comment">Comment: Can be a partial date, for example, only a year.</items>
                        </term_definitions>
                        <term_definitions code="at0150">
                            <items id="text">Overall details</items>
                            <items id="description">Additional structured details about the overall use of contraception.</items>
                        </term_definitions>
                        <term_definitions code="at0151">
                            <items id="text">Type</items>
                            <items id="description">The type of contraception used by the individual.</items>
                            <items id="comment">Coding the contraceptive type with a terminology is recommended, where possible.</items>
                        </term_definitions>
                        <term_definitions code="at0152">
                            <items id="text">Male condom</items>
                            <items id="description">A barrier covering the penis to physically block ejaculated sperm from entering the vagina.</items>
                        </term_definitions>
                        <term_definitions code="at0153">
                            <items id="text">Female condom</items>
                            <items id="description">A barrier worn inside the vagina to physically block ejaculated sperm from entering the cervix.</items>
                        </term_definitions>
                        <term_definitions code="at0154">
                            <items id="text">Diaphragm</items>
                            <items id="description">A shallow, dome shaped barrier placed in the vagina to cover the cervix.</items>
                        </term_definitions>
                        <term_definitions code="at0155">
                            <items id="text">Combination pill</items>
                            <items id="description">A combination oral contraceptive pill containing both an oestrogen and progestin.</items>
                        </term_definitions>
                        <term_definitions code="at0156">
                            <items id="text">Combination skin patch</items>
                            <items id="description">A combination skin patch containing both an oestrogen and progestin.</items>
                        </term_definitions>
                        <term_definitions code="at0157">
                            <items id="text">Progestogen-only pill</items>
                            <items id="description">Progestogen-only form of oral hormonal contraception.</items>
                        </term_definitions>
                        <term_definitions code="at0158">
                            <items id="text">Depot progestogen injection</items>
                            <items id="description">Progestogen-only injectable of hormonal contraception.</items>
                        </term_definitions>
                        <term_definitions code="at0159">
                            <items id="text">Hormone implant</items>
                            <items id="description">Hormonal contraception inserted under the skin.</items>
                        </term_definitions>
                        <term_definitions code="at0160">
                            <items id="text">Vaginal ring</items>
                            <items id="description">Hormone impregnated ring inserted into the vagina.</items>
                        </term_definitions>
                        <term_definitions code="at0161">
                            <items id="text">Female sterilisation</items>
                            <items id="description">Procedure intended to permanently prevent pregnancy, usually by tubal ligitation.</items>
                        </term_definitions>
                        <term_definitions code="at0162">
                            <items id="text">Male sterilisation</items>
                            <items id="description">Procedure intended to permanently prevent pregnancy, usually by vasectomy.</items>
                        </term_definitions>
                        <term_definitions code="at0163">
                            <items id="text">IUD without hormone</items>
                            <items id="description">Device inserted into the uterus to prevent implantation.</items>
                        </term_definitions>
                        <term_definitions code="at0164">
                            <items id="text">Withdrawal</items>
                            <items id="description">Withdrawing the penis from the vagina and away from a woman's external genitals before ejaculation.</items>
                        </term_definitions>
                        <term_definitions code="at0165">
                            <items id="text">Fertility awareness method</items>
                            <items id="description">Cyclical use of abstinence or barrier methods during likely fertile periods.</items>
                        </term_definitions>
                        <term_definitions code="at0166">
                            <items id="text">Abstinence</items>
                            <items id="description">Avoidance of vaginal intercourse or intimate activities which may result in pregnancy.</items>
                        </term_definitions>
                        <term_definitions code="at0167">
                            <items id="text">Vaginal douching</items>
                            <items id="description">Intravaginal cleansing with a liquid solution.</items>
                        </term_definitions>
                        <term_definitions code="at0168">
                            <items id="text">Clinical indication</items>
                            <items id="description">Clinical reason for using the specified type of contraception during this episode.</items>
                            <items id="comment">For example: dysmenorrhoea; or irregular menses. Coding with an external terminology is preferred, where possible.</items>
                        </term_definitions>
                        <term_definitions code="at0169">
                            <items id="text">IUD with hormone</items>
                            <items id="description">Device inserted into the uterus to prevent implantation.</items>
                        </term_definitions>

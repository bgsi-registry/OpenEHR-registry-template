                        <archetype_id>
                            <value>openEHR-EHR-ADMIN_ENTRY.episode_institution.v0</value>
                        </archetype_id>
                        <term_definitions code="at0000">
                            <items id="text">Episode of care - institution</items>
                            <items id="description">Administrative details about a period of admitted patient care between a formal or statistical admission and a formal or statistical separation, characterised by only one care type of care from a healthcare institution.</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                            <items id="text">Item tree</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                            <items id="text">Tanggal lepas perawatan</items>
                            <items id="description">The date and/or time of the formal or statistical end of the admission to the institution.</items>
                            <items id="comment">For example: transfer of care to home or another institution; or date of death.&#13;
Formal recording of the date of death should use CLUSTER.death_details.</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                            <items id="text">Destination category</items>
                            <items id="description">The type of residence where the individual resided after the episode of care.</items>
                            <items id="comment">For example: home; hospital; nursing home; or rehabilitation hospital.</items>
                        </term_definitions>
                        <term_definitions code="at0004">
                            <items id="text">Tanggal admisi RS</items>
                            <items id="description">The date and/or time of the formal or statistical admission to the institution.</items>
                        </term_definitions>
                        <term_definitions code="at0006">
                            <items id="text">Luaran saat lepas perawatan</items>
                            <items id="description">Outcome for the individual at the end of the episode of care.</items>
                            <items id="comment">For example: recovered/not recovered/death.</items>
                        </term_definitions>
                        <term_definitions code="at0007">
                            <items id="text">Source category</items>
                            <items id="description">The type of residence where the individual resided prior to admission.</items>
                            <items id="comment">For example: hospital, community, or nursing home. 'Admission source' should be coded with a terminology, where possible.</items>
                        </term_definitions>
                        <term_definitions code="at0008">
                            <items id="text">Reason for admission</items>
                            <items id="description">The reason why the individual was admitted for an episode of care.</items>
                            <items id="comment">For example: respite; rehabilitation; or infection isolation.&#13;
Diagnosies should be recorded using the EVAL.problem_diagnosis archetype plus the nested CLUSTER.problem_qualifier, if necessary.</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                            <items id="text">Admission category</items>
                            <items id="description">The type of admission.</items>
                            <items id="comment">For example: day surgery; elective surgery or em. </items>
                        </term_definitions>
                        <term_definitions code="at0010">
                            <items id="text">Comment</items>
                            <items id="description">Additional narrative about the episode, not captured in other fields.</items>
                        </term_definitions>
                        <term_definitions code="at0011">
                            <items id="text">Additional details</items>
                            <items id="description">Additional structured details about the episode of care within an institution.</items>
                            <items id="comment">For example: </items>
                        </term_definitions>
                        <term_definitions code="at0012">
                            <items id="text">Destination</items>
                            <items id="description">The organisation or address for the individual after the admission.</items>
                        </term_definitions>
                        <term_definitions code="at0013">
                            <items id="text">Source</items>
                            <items id="description">The organisation or address for the individual prior to the admission.</items>
                        </term_definitions>
                        <term_definitions code="at0014">
                            <items id="text">Episode ID</items>
                            <items id="description">An identifier for the episode of care.</items>
                        </term_definitions>
                        <term_definitions code="at0015">
                            <items id="text">Attending clinicians</items>
                            <items id="description">Details about the consultant and attending clinicians responsible for care during the episode.</items>
                        </term_definitions>
                        <term_definitions code="at0016">
                            <items id="text">Nomor rekam medis</items>
                            <items id="description">Identification of the medical record number.</items>
                        </term_definitions>
                        <term_definitions code="at0017">
                            <items id="text">Location</items>
                            <items id="description">An organisation, physical address or structured address describing where the individual was located during the episode of care.</items>
                            <items id="comment">For example: 2nd floor, Moorhouse wing, Turnbull building, General Hospital.</items>
                        </term_definitions>
                        <term_definitions code="at0018">
                            <items id="text">Physical location</items>
                            <items id="description">The physical location of the individual within the institution during the episode of care.</items>
                        </term_definitions>
                        <term_definitions code="at0019">
                            <items id="text">Location onset</items>
                            <items id="description">The date the individual was moved to the internal institution location.</items>
                            <items id="comment">For example, the date moved to a new ward or bed.</items>
                        </term_definitions>
                        <term_definitions code="at0020">
                            <items id="text">Location end</items>
                            <items id="description">The date the individual was moved from the internal institution location.</items>
                        </term_definitions>
                        <term_definitions code="at0021">
                            <items id="text">Attending unit</items>
                            <items id="description">The clinical service unit assigned for care of the individual.</items>
                            <items id="comment">For example: Orthopaedic A unit.</items>
                        </term_definitions>
                        <term_definitions code="at0022">
                            <items id="text">Ward</items>
                            <items id="description">The name of the Ward within the institution where the individual was located.</items>
                        </term_definitions>
                        <term_definitions code="at0023">
                            <items id="text">Room</items>
                            <items id="description">The name of the Room within the institution where the individual was located.</items>
                        </term_definitions>
                        <term_definitions code="at0024">
                            <items id="text">Bed</items>
                            <items id="description">The name of the Bed within the institution where the individual was located.</items>
                        </term_definitions>
                        <term_definitions code="at0025">
                            <items id="text">Location category</items>
                            <items id="description">The type of location.</items>
                            <items id="comment">For example: Intensive care unit; high dependency unit.</items>
                        </term_definitions>
                        <term_definitions code="at0026">
                            <items id="text">Kelas perawatan</items>
                            <items id="description">The type of health insurance applicable for the episode.</items>
                        </term_definitions>

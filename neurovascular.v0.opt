                        <term_definitions code="at0000">
                            <items id="text">HAS-BLED score</items>
                            <items id="description">A risk assessment score to assess the bleeding risk in individuals with atrial fibrillation.</items>
                            <items id="comment">The HAS-BLED mnemonic represents each risk factor:&#13;
- Hypertension,&#13;
- Abnormal renal/liver function, &#13;
- Stroke, &#13;
-Bleeding history or predisposition, &#13;
- Labile INR, &#13;
- Elderly (&gt;65), and&#13;
- Drugs/alcohol concomitantly.</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                            <items id="text">Event Series</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                            <items id="text">Any point in time event</items>
                            <items id="description">Default, unspecified point in time event which may be explicitly defined in a template or at run-time.</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                            <items id="text">Tree</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0004">
                            <items id="text">Hypertension</items>
                            <items id="description">The systolic blood pressure is &gt;160 mmHg.</items>
                        </term_definitions>
                        <term_definitions code="at0005">
                            <items id="text">Abnormal renal function</items>
                            <items id="description">The presence of chronic dialysis or renal transplantation or serum creatinine 200mol/L.</items>
                        </term_definitions>
                        <term_definitions code="at0006">
                            <items id="text">Abnormal liver function</items>
                            <items id="description">Chronic hepatic disease (eg. cirrhosis) or biochemical evidence of significant hepatic derangement (eg. bilirubin &gt;2x upper limit of normal, in association with AST/ALT/ALP &gt;3x upper limit normal, etc).</items>
                        </term_definitions>
                        <term_definitions code="at0007">
                            <items id="text">Stroke</items>
                            <items id="description">A previous history of stroke, particularly lacunar.</items>
                        </term_definitions>
                        <term_definitions code="at0008">
                            <items id="text">Bleeding history or predisposition</items>
                            <items id="description">A previous bleeding history and/or predisposition to bleeding .</items>
                            <items id="comment">For example: bleeding diathesis, anaemia, etc.</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                            <items id="text">Labile INR</items>
                            <items id="description">Unstable/high INRs or poor time in therapeutic range.</items>
                            <items id="comment">For example: &lt;60%.</items>
                        </term_definitions>
                        <term_definitions code="at0010">
                            <items id="text">Elderly</items>
                            <items id="description">Age &gt;65 yrs.</items>
                        </term_definitions>
                        <term_definitions code="at0011">
                            <items id="text">Drugs concomitantly</items>
                            <items id="description">The concomitant use of drugs, such as antiplatelet agents, non-steroidal anti-inflammatory drugs, etc.</items>
                        </term_definitions>
                        <term_definitions code="at0012">
                            <items id="text">Alcohol concomitantly</items>
                            <items id="description">Alcohol consumption  8 units per week.</items>
                        </term_definitions>
                        <term_definitions code="at0013">
                            <items id="text">HAS-BLED score</items>
                            <items id="description">Sum of points assigned for each of the component parameters.</items>
                        </term_definitions>
                        <term_definitions code="at0014">
                            <items id="text">No</items>
                            <items id="description">The risk factor is not present.</items>
                        </term_definitions>
                        <term_definitions code="at0015">
                            <items id="text">Yes</items>
                            <items id="description">The risk factor is present.</items>
                        </term_definitions>
                        <term_definitions code="at0017">

                        <term_definitions code="at0000">
                            <items id="text">Family history screening questionnaire</items>
                            <items id="description">An individual- or self-reported questionnaire screening for significant health-related problems found in family members.</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                            <items id="text">History</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                            <items id="text">Any event</items>
                            <items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                            <items id="text">Tree</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0004">
                            <items id="text">Screening purpose</items>
                            <items id="description">The reason for overall screening.</items>
                            <items id="comment">For example: pre-operative screening.</items>
                        </term_definitions>
                        <term_definitions code="at0005">
                            <items id="text">Any presence?</items>
                            <items id="description">Presence of any relevant problems in the family.</items>
                        </term_definitions>
                        <term_definitions code="at0006">
                            <items id="text">Yes</items>
                            <items id="description">Problems are present.</items>
                        </term_definitions>
                        <term_definitions code="at0007">
                            <items id="text">No</items>
                            <items id="description">Problems are not present.</items>
                        </term_definitions>
                        <term_definitions code="at0008">
                            <items id="text">Unknown</items>
                            <items id="description">It is not known whether any problems are present or absent.</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                            <items id="text">Specific family health problem</items>
                            <items id="description">Screening details about a specified health problem.</items>
                            <items id="comment">This grouping is currently  ordered to indicate relationships per problem, but in practice could also be used to capture problems per relationship as Problem/Diagnosis name and Relationship both allow multiple occurrences.</items>
                        </term_definitions>
                        <term_definitions code="at0010">
                            <items id="text">Present?</items>
                            <items id="description">Presence of any relevant specific problem.</items>
                        </term_definitions>
                        <term_definitions code="at0011">
                            <items id="text">Yes</items>
                            <items id="description">The specific problem is present.</items>
                        </term_definitions>
                        <term_definitions code="at0012">
                            <items id="text">No</items>
                            <items id="description">The specific problem is absent.</items>
                        </term_definitions>
                        <term_definitions code="at0013">
                            <items id="text">Unknown</items>
                            <items id="description">It is not known whether any problems are present or absent.</items>
                        </term_definitions>
                        <term_definitions code="at0018">
                            <items id="text">Relationship</items>
                            <items id="description">The relationship of the family member to the individual.&#13;
</items>
                            <items id="comment">For example: mother, step-father, maternal grandmother, or paternal uncle. Coding of the relationship with a terminology is preferred, where possible and including specification of maternal and paternal as required.</items>
                        </term_definitions>
                        <term_definitions code="at0021">
                            <items id="text">Item tree</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0028">
                            <items id="text">Comment</items>
                            <items id="description">Additional narrative about the specified problem or family member, not captured in other fields.</items>
                        </term_definitions>
                        <term_definitions code="at0029">
                            <items id="text">Extension</items>
                            <items id="description">Additional information required to capture local content or to align with other reference models/formalisms.</items>
                            <items id="comment">For example: local information requirements or additional metadata to align with FHIR.</items>
                        </term_definitions>
                        <term_definitions code="at0035">
                            <items id="text">Riwayat kesehatan pada keluarga</items>
                            <items id="description">Identification of the significant problem or diagnosis in the identified family member.</items>
                            <items id="comment">Coding of the family member's problem or diagnosis with a terminology is preferred, where possible. </items>
                        </term_definitions>
                        <term_definitions code="at0036">
                            <items id="text">Screening details</items>
                            <items id="description">Additional details or questions  about the specific problem or family member.</items>
                        </term_definitions>
                        <term_definitions code="at0037">
                            <items id="text">Age at diagnosis</items>
                            <items id="description">Age of the family member at diagnosis.</items>
                        </term_definitions>
                        <term_definitions code="local_terms::75104002">
                            <items id="text">Riwayat stroke keluarga</items>
                        </term_definitions>
